
@{
    ViewData["Title"] = "Üyelik Seçenekleri";
    ViewData["Description"] = "Yazgelder - Yazılım Geliştiricileri Derneği" +ViewData["Title"];
    ViewData["Url"] = "https://www.yazgelder.org/Member";
    ViewData["Date"] = (DateTime.Now.ToString("s")) + "+0300";
    ViewData["image"] = "https://www.yazgelder.org/Images/ShareImage.png";
    ViewData["image:width"] = "1200";
    ViewData["image:height"] = "627";
}
@section Haders{


}

<section id="sub-header-3" data-speed="8" data-type="background" class="padding-top-bottom subheader" style="background-position: 50% 0px;">
    <div class="container">
        <div class="row">
            <div class="col-lg-9 col-md-9 col-sm-9">
                <h1>@ViewData["Title"]</h1>
            </div>
            <div class="breadcrumbs-outer">
                <ul class="breadcrumbs">
                    <li><a href="/" title="Yazılım Geliştiricileri Derneği">Ana Sayfa</a></li>
                    <li class="active">@ViewData["Title"]</li>
                </ul>
            </div>
        </div>
    </div>
</section>



<div id="content" class="no-padding">


    <section id="section-about">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-sm-6">
                    <img src="/images/Uyelik.jpg" alt="Yazılım Geliştiricileri Derneği - @ViewData["Title"]" class="img-responsive">
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="text-center">
                                <h2 class="box-title">Üyelik Seçenekleri</h2>
                                <div class="tiny-border"></div>
                            </div>
                        </div>
                        <div class="col-md-6 ">
                            <div class="pricing-item featured">

                                <div class="plan-name">Fahri Üye</div>
                                <div class="plan-details">
                                    <ul>
                                        <li><b>Evet</b> Projelere Katılma</li>
                                        <li><b>Evet</b> Sms Alma</li>
                                        <li><b>Hayır</b> Oy Kullanma</li>
                                        <li><b>Hayır</b> Projelerde Ayrıcalıklar</li>
                                        <li><b>Ücretsiz</b> Danışmanlık</li>
                                        <li><b>Ücretli</b> Aktiviteler</li>
                                        <li><b>Ucretsiz</b> Katılım Ücreti</li>
                                        <li><b>Ucretsiz</b> Yıllık Aidat</li>
                                    </ul>
                                </div>
                                <p class="plan-btn"><a href="/member/Honorary" class="btn btn-primary">Kayıt Ol</a></p>
                            </div>
                        </div>
                        <div class="col-md-6 ">
                            <div class="pricing-item">

                                <div class="plan-name">Genel Kurul Üyesi</div>
                                <div class="plan-details">
                                    <ul>
                                        <li><b>Evet</b> Projelere Katılma</li>
                                        <li><b>Evet</b> Sms Alma</li>
                                        <li><b>Evet</b> Oy Kullanma</li>
                                        <li><b>Evet</b> Projelerde Ayrıcalıklar</li>
                                        <li><b>Ücretsiz</b> Danışmanlık</li>
                                        <li><b>Ücretsiz</b> Aktiviteler</li>
                                        <li><b>50₺</b> Kayıt Ücreti</li>
                                        <li><b>100₺</b> Yıllık Aidat</li>
                                    </ul>
                                </div>
                                <p class="plan-btn"><a href="/member/GeneralAssembly" class="btn btn-primary">Kayıt Ol</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


    @Html.Partial("_fooder")

</div>

